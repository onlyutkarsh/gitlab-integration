{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/*.js.map": true,
    "**/*.taskkey": true,
    "**/*.js": {
      "when": "$(basename).ts"
    }
  },
  "editor.formatOnSave": true,
  "workbench.colorCustomizations": {
    "tab.activeBorder": "#23c1ff",
  }
}